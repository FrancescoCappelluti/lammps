units		real	
atom_style	full
boundary   p p p
dimension 3

read_data Ice-Ic-40A-box.dat
atom_modify sort 0 0 #Maybe not necessary
neigh_modify one 2500 page 125000

pair_style	lj/cut/coul/long 13
kspace_style	pppm 1.0e-6

pair_coeff	2 2 0.1521 3.1507
pair_coeff	* 1 0.0000 0.0000	

bond_style	harmonic
angle_style	harmonic

bond_coeff	1 450.00 0.9572
angle_coeff	1 55.0 104.52

neighbor        2.0 bin
neigh_modify	every 1 delay 8 check yes

velocity all create 1.0 4928229 mom yes rot yes dist gaussian

fix     1 all fresp/dsf 1 12.5 0.095 fresp_types.dat polarparm.dat damp exp 0.3 2.0
fix		2 all nvt temp 1 200 200
fix		4 all shake 0.0001 20 0 b 1 a 1

timestep	2.0

thermo 50

restart 5000 restart1 restart2

run	1000
